<odoo>
    <data>
        <record id="ir_cron_archive_presale_orders" model="ir.cron">
            <field name="name">Archive Confirmed Presale Orders</field>
            <field name="model_id" ref="x_model_presale_order"/>
            <field name="state">code</field>
            <field name="code">
env["x_presale.order"].search([('x_state', '!=', 'new')]).write({'x_active': False})
            </field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>
